<!DOCTYPE struts-config PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://jakarta.apache.org/struts/dtds/struts-2.5.dtd">


<struts-config>

    <!-- ========== Form Bean Definitions =================================== -->
    <form-beans>
        <!-- XWiki Struts Application -->
        <form-bean name="edit"
                   type="com.xpn.xwiki.web.EditForm"/>
        <form-bean name="inline"
                   type="com.xpn.xwiki.web.EditForm"/>
        <form-bean name="lock"
                   type="com.xpn.xwiki.web.EditForm"/>
        <form-bean name="preview"
                   type="com.xpn.xwiki.web.EditForm"/>
        <form-bean name="save"
                   type="com.xpn.xwiki.web.EditForm"/>
        <form-bean name="commentsave"
                   type="com.xpn.xwiki.web.EditForm"/>
        <form-bean name="saveandcontinue"
                   type="com.xpn.xwiki.web.EditForm"/>
        <form-bean name="rollback"
                   type="com.xpn.xwiki.web.RollbackForm"/>
        <form-bean name="cancel"
                   type="com.xpn.xwiki.web.EditForm"/>
        <form-bean name="propupdate"
                   type="com.xpn.xwiki.web.EditForm"/>
        <form-bean name="propadd"
                   type="com.xpn.xwiki.web.PropAddForm"/>
        <form-bean name="propdisable"
                   type="com.xpn.xwiki.web.PropChangeForm"/>
        <form-bean name="propenable"
                   type="com.xpn.xwiki.web.PropChangeForm"/>
        <form-bean name="propdelete"
                   type="com.xpn.xwiki.web.PropChangeForm"/>
        <form-bean name="objectadd"
                   type="com.xpn.xwiki.web.ObjectAddForm"/>
        <form-bean name="commentadd"
                   type="com.xpn.xwiki.web.ObjectAddForm"/>
        <form-bean name="objectremove"
                   type="com.xpn.xwiki.web.ObjectRemoveForm"/>
        <form-bean name="admin"
                   type="com.xpn.xwiki.web.EditForm"/>
        <form-bean name="deleteversions"
                   type="com.xpn.xwiki.web.DeleteVersionsForm"/>
    </form-beans>

    <!-- ========== Global Forward Definitions ============================== -->
    <global-forwards>

        <!-- Forward to view -->
        <forward
                name="start"
                path="/view/Main/WebHome"/>

        <forward name="login" path="/templates/login.vm"/> <!-- not enough -->
        <forward name="accessdenied" path="/templates/accessdenied.vm"/>
        <forward name="userinactive" path="/templates/userinactive.vm"/>
    </global-forwards>


    <!-- ========== Action Mapping Definitions ============================== -->

    <!-- XWiki Struts Application with Velocity -->
    <action class="com.xpn.xwiki.web.ViewAction"
            name="view">
        <result name="view" type="velocity">/templates/view.vm</result>
        <result name="notexist" type="velocity">/templates/notexist.vm</result>
        <result name="plain" type="velocity">/templates/plain.vm</result>
    </action>
    <action class="com.xpn.xwiki.web.ViewAttachRevAction"
            name="viewattachrev">
        <result name="viewattachrev" type="velocity">/templates/viewattachrev.vm</result>
    </action>

    <action class ="com.xpn.xwiki.web.ViewrevAction"
            name="viewrev">
        <result name="notexist" type="velocity">/templates/notexist.vm</result>
        <result name="view" type="velocity">/templates/view.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.PDFAction"
            name="pdf">
    </action>
    <action class="com.xpn.xwiki.web.TexAction"
            name="tex">
    </action>
    <action class="com.xpn.xwiki.web.EditAction"
            name="edit">
        <result name="edit" type="velocity">/templates/edit.vm</result>
    </action>
    <action class="com.xpn.xwiki.web.CreateAction"
            name="create">
        <result name="create" type="velocity">/templates/create.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.InlineAction"
            name="inline">
        <result name="inline" type="velocity">/templates/inline.vm</result>
        <result name="pageexist" type="velocity">/templates/inline.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.PreviewAction"
            name="preview">
        <result name="preview" type="velocity">/templates/preview.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.SaveAction"
            name="save">
        <result name="save" type="velocity">/templates/save.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.SaveAndContinueAction"
            name="saveandcontinue" oldname="save">
        <result name="save" type="velocity">/templates/save.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.RollbackAction"
            name="rollback">
        <result name="edit" type="velocity">/templates/rollback.vm</result>
        <result name="rollback" type="velocity">/templates/rollback.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.DeleteVersionsAction"
            name="deleteversions">
    </action>

    <action class="com.xpn.xwiki.web.CancelAction"
            name="cancel">
        <result name="cancel" type="velocity">/templates/cancel.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.DeleteAction"
            name="delete">
        <result name="delete" type="velocity">/templates/delete.vm</result>
        <result name="deleted" type="velocity">/templates/deleted.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.DeleteSpaceAction"
            name="deletespace">
    </action>

    <action class="com.xpn.xwiki.web.UndeleteAction"
            name="undelete">
    </action>

    <action class="com.xpn.xwiki.web.ResetVersionsAction"
            name="reset">
        <result name="delete" type="velocity">/templates/reset.vm</result>
        <result name="deleted" type="velocity">/templates/reset.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.RegisterAction"
            name="register">
        <result name="register" type="velocity">/templates/register.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.PropUpdateAction"
            name="propupdate">
        <result name="propupdate" type="velocity">/templates/edit.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.PropAddAction"
            name="propadd">
        <result name="propadd" type="velocity">/templates/edit.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.PropDisableAction"
            name="propdisable">
    </action>

    <action class="com.xpn.xwiki.web.PropEnableAction"
            name="propenable">
    </action>

    <action class="com.xpn.xwiki.web.PropDeleteAction"
            name="propdelete">
        <result name="propdelete" type="velocity">/templates/edit.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.ObjectAddAction"
            name="objectadd">
        <result name="objectadd" type="velocity">/templates/edit.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.CommentAddAction"
            name="commentadd">
        <result name="commentadd" type="velocity">/templates/edit.vm</result>
    </action>
    <action class="com.xpn.xwiki.web.CommentSaveAction"
            name="commentsave">
        <result name="commentsave" type="velocity">/templates/edit.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.ObjectsSynchronizeAction"
            name="objectsync">
    </action>
    <action class="com.xpn.xwiki.web.ObjectRemoveAction"
            name="objectremove">
        <result name="objectremove" type="velocity">/templates/edit.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.AttachAction"
            name="attach">
        <result name="attach" type="velocity">/templates/attachments.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.UploadAction"
            name="upload">
        <result name="upload" type="velocity">/templates/attachments.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.DownloadAction"
            name="download">
    </action>
    <action class="com.xpn.xwiki.web.TempResourceAction"
            name="temp">
    </action>
    <action class="com.xpn.xwiki.web.DownloadRevAction"
            name="downloadrev">
    </action>
    <action class="com.xpn.xwiki.web.DotAction"
            name="dot">
    </action>

    <action class="com.xpn.xwiki.web.SVGAction"
            name="svg">
    </action>

    <action class="com.xpn.xwiki.web.DeleteAttachmentAction"
            name="delattachment">
    </action>

    <action class="com.xpn.xwiki.web.SkinAction"
            name="skin">
    </action>

    <action class="com.xpn.xwiki.web.JsxAction"
            name="jsx">
    </action>

    <action class="com.xpn.xwiki.web.SsxAction"
            name="ssx">
    </action>

    <action class="com.xpn.xwiki.web.LoginAction"
            name="login">
    </action>

    <action class="com.xpn.xwiki.web.LoginSubmitAction"
            name="loginsubmit">
    </action>

    <action class="com.xpn.xwiki.web.LoginErrorAction"
            name="loginerror">
    </action>

    <action class="com.xpn.xwiki.web.LogoutAction"
            name="logout">
        <result name="logout" type="velocity">/templates/logout.vm</result>
    </action>

    <action class="com.xpn.xwiki.plugin.charts.actions.ChartingAction"
            name="charting">
    </action>

    <action class="com.xpn.xwiki.web.LockAction"
            name="lock">
    </action>

    <action class="com.xpn.xwiki.web.RedirectAction"
            name="redirect">
    </action>

    <action class="com.xpn.xwiki.web.AdminAction"
            name="admin">
        <result name="admin" type="velocity">/templates/admin.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.ExportAction"
            name="export">
        <result name="export" type="velocity" type="velocity">/templates/export.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.ImportAction"
            name="import">
        <result name="import" type="velocity">/templates/import.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.GetAction"
            name="get">
        <result name="get" type="velocity">/templates/get.vm</result>
    </action>

    <action class="org.xwiki.extension.distribution.internal.DistributionAction"
            name="distribution">
        <result name="distribution" type="velocity">/templates/distribution.vm</result>
    </action>

    <action class="com.xpn.xwiki.web.UnknownAction"
            name="unknown"
            unknown="true">
    </action>

    <controller processorClass="com.xpn.xwiki.web.XWikiRequestProcessor"
                debug="0" contentType="text/html"/>

</struts-config>
